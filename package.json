{
  "name": "jekyll-utils",
  "version": "0.1.4",
  "license": "MIT",
  "description": "A collection of Jekyll utilility methods to generate eg. Jekyll permalinks and slugs",
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "homepage": "https://github.com/voxpelli/node-jekyll-utils",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-jekyll-utils.git"
  },
  "main": "index.js",
  "scripts": {
    "mocha": "NODE_ENV=test nyc --reporter=lcov --reporter text mocha test/**/*.spec.js",
    "dependency-check": "JS_FILES=\"*.js test/*.js\" && dependency-check . $JS_FILES && dependency-check . $JS_FILES --unused --no-dev",
    "test": "installed-check -e && eslint . && npm run dependency-check && npm run mocha && documentation readme -s \"API Usage\" --no-markdown-toc --diff-only",
    "prepush": "npm test",
    "build-docs": "documentation readme -s \"API Usage\" --no-markdown-toc"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.2",
    "dependency-check": "3.2.0",
    "documentation": "8.1.0",
    "eslint": "4.19.1",
    "eslint-config-semistandard": "12.0.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "husky": "0.14.3",
    "installed-check": "2.2.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2"
  },
  "dependencies": {
    "strftime": "^0.10.0",
    "xregexp": "^4.0.0"
  }
}
