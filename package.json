{
  "name": "jekyll-utils",
  "version": "0.1.4",
  "license": "MIT",
  "description": "A collection of Jekyll utilility methods to generate eg. Jekyll permalinks and slugs",
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "homepage": "https://github.com/voxpelli/node-jekyll-utils",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-jekyll-utils.git"
  },
  "main": "index.js",
  "scripts": {
    "mocha": "NODE_ENV=test nyc --reporter=lcov --reporter text mocha test/**/*.spec.js",
    "dependency-check": "JS_FILES=\"*.js test/*.js\" && dependency-check . $JS_FILES && dependency-check . $JS_FILES --unused --no-dev",
    "test": "installed-check -e -i eslint && eslint . && npm run dependency-check && npm run mocha && documentation readme -s \"API Usage\" --no-markdown-toc --diff-only",
    "build-docs": "documentation readme -s \"API Usage\" --no-markdown-toc"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "dependency-check": "3.2.1",
    "documentation": "9.1.1",
    "eslint": "5.11.0",
    "eslint-config-semistandard": "13.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "1.3.1",
    "installed-check": "2.2.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0"
  },
  "dependencies": {
    "strftime": "^0.10.0",
    "xregexp": "^4.0.0"
  }
}
